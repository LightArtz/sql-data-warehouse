/*
======================================
Create Database and Schemas
======================================

This script creates a new database named 'DataWarehouse' and sets up three schemas within the database (bronze, silver, gold).
*/


USE master;
GO

-- Create the 'DataWarehouse' database
CREATE DATABASE DataWarehouse;
GO

USE DataWarehouse;
GO

-- Create Schemas
CREATE SCHEMA bronze;
GO
CREATE SCHEMA silver;
GO
CREATE SCHEMA gold;
GO
